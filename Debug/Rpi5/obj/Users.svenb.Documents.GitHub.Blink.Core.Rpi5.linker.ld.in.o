       
       
CPU_COUNT = 4;;
PAGE_TABLE_SIZE = 4096;;
STACK_SIZE = 4096;;
SECTIONS
{
. = 0x80000;
.text :
 {
 KEEP(*(.text.start))
 *(.text .text.*)
 }
.got :
 {
 KEEP(*(.got .got.*))
 }
.eh_frame :
 {
 KEEP(*(.eh_frame*))
 }
.gcc_except_table :
 {
 KEEP(*(.gcc_except_table .gcc_except_table.*))
 }
.init_array :
 {
 __init_array_start = .;
 KEEP(*(.init_array*))
 __init_array_end = .;
 }
.data :
 {
 *(.rodata .rodata.*)
 *(.data .data.*)
 }
.bss (NOLOAD) :
 {
 . = ALIGN(8);
 __bss_start = .;
 *(.bss .bss.*)
 *(COMMON)
 . = ALIGN(8);
 __bss_end = .;
 }
.stack (NOLOAD) :
 {
 . = ALIGN(STACK_SIZE);
 __stack_start = .;
 . += CPU_COUNT * STACK_SIZE;
 __stack_end = .;
 }
.page_table (NOLOAD) :
 {
 . = ALIGN(PAGE_TABLE_SIZE);
 __page_table_start = .;
 . += PAGE_TABLE_SIZE;
 __page_table_end = .;
 }
__end = .;
__heap_start = .;
/DISCARD/ :
 {
 *(.comment)
 *(.dtors)
 *(.eh_frame*)
 }
}
