//=========
// cstddef
//=========

#pragma once


//========
// Macros
//========

#ifndef __long
#define __long
#endif

#ifndef __long_long
#define __long_long long
#endif


//===========
// Namespace
//===========

namespace std {


//========
// Common
//========

typedef __long_long int ptrdiff_t;
typedef unsigned __long_long size_t;

}
