//=======
// cmath
//=======

#pragma once


//===========
// Namespace
//===========

namespace std {


//========
// Common
//========

constexpr bool isinf(float f) { return __builtin_isinf(f); }
constexpr bool isinf(double f) { return __builtin_isinf(f); }
constexpr bool isnan(float f) { return __builtin_isnan(f); }
constexpr bool isnan(double f) { return __builtin_isnan(f); }

}