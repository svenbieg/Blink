//=====
// new
//=====

#pragma once


//=======
// Using
//=======

#include "Platform.h"


//==============
// Operator New
//==============

void* operator new(__size_t size);
void* operator new[](__size_t size);
void operator delete(void* buf)noexcept;
void operator delete(void* buf, __size_t size)noexcept;
void operator delete[](void* buf)noexcept;
void operator delete[](void* buf, __size_t size)noexcept;


//===============
// Placement New
//===============

inline void* operator new(__size_t size, void* obj) { return obj; }
